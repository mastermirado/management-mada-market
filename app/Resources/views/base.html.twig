<!-- === BEGIN HEADER === -->
<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<head>
    <!-- Title -->
    <title>Lifestyle - Professional Bootstrap Template</title>
    <!-- Meta -->
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <!-- Favicon -->
    <link href="favicon.ico" rel="shortcut icon">

    {% block stylesheets %}
        <!-- Bootstrap Core CSS -->
        <link rel="stylesheet" href="{{ asset('bundles/mmmfrontend/css/bootstrap.css') }}" rel="stylesheet">
        <!-- Template CSS -->
        <link rel="stylesheet" href="{{ asset('bundles/mmmfrontend/css/animate.css') }}" rel="stylesheet">
        <link rel="stylesheet" href="{{ asset('bundles/mmmfrontend/css/font-awesome.css') }}" rel="stylesheet">
        <link rel="stylesheet" href="{{ asset('bundles/mmmfrontend/css/nexus.css') }}" rel="stylesheet">
        <link rel="stylesheet" href="{{ asset('bundles/mmmfrontend/css/responsive.css') }}" rel="stylesheet">
        <link rel="stylesheet" href="{{ asset('bundles/mmmfrontend/css/custom.css') }}" rel="stylesheet">
    {% endblock %}

    <!-- Google Fonts-->
    <link href="http://fonts.googleapis.com/css?family=Roboto+Condensed:400,300" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=PT+Sans" type="text/css" rel="stylesheet">
    <link href="http://fonts.googleapis.com/css?family=Roboto:400,300" rel="stylesheet" type="text/css">
</head>
<body>
<div id="body-bg">
    {% block social %}
        {% include 'MMMFrontEndBundle:Static:social.html.twig' %}
    {% endblock %}

    {% block preheader %}
        {% include 'MMMFrontEndBundle:Static:preheader.html.twig' %}
    {% endblock %}

    {% block menu %}
        {{ render(controller('MMMFrontEndBundle:Static:menu')) }}
    {% endblock %}

    {% block postheader %}
        {% include 'MMMFrontEndBundle:Static:postheader.html.twig' %}
    {% endblock %}

    {% block body %}
    {% endblock %}

    {% block footer %}
        {% include 'MMMFrontEndBundle:Static:footer.html.twig' %}
    {% endblock %}


    {% block javascripts %}
        <!-- JS -->
        <script type="text/javascript" src="{{ asset('bundles/mmmfrontend/js/jquery.min.js') }}" type="text/javascript"></script>
        <script type="text/javascript" src="{{ asset('bundles/mmmfrontend/js/bootstrap.min.js') }}" type="text/javascript"></script>
        <script type="text/javascript" src="{{ asset('bundles/mmmfrontend/js/scripts.js') }}"></script>
        <!-- Isotope - Portfolio Sorting -->
        <script type="text/javascript" src="{{ asset('bundles/mmmfrontend/js/jquery.isotope.js') }}" type="text/javascript"></script>
        <!-- Mobile Menu - Slicknav -->
        <script type="text/javascript" src="{{ asset('bundles/mmmfrontend/js/jquery.slicknav.js') }}" type="text/javascript"></script>
        <!-- Animate on Scroll-->
        <script type="text/javascript" src="{{ asset('bundles/mmmfrontend/js/jquery.visible.js') }}" charset="utf-8"></script>
        <!-- Sticky Div -->
        <script type="text/javascript" src="{{ asset('bundles/mmmfrontend/js/jquery.sticky.js') }}" charset="utf-8"></script>
        <!-- Slimbox2-->
        <script type="text/javascript" src="{{ asset('bundles/mmmfrontend/js/slimbox2.js') }}" charset="utf-8"></script>
        <!-- Modernizr -->
        <script src="{{ asset('bundles/mmmfrontend/js/modernizr.custom.js') }}" type="text/javascript"></script>
        <!-- End JS -->
    {% endblock %}

</div>
</body>
</html>